@if(Request.IsAuthenticated)
{
    <text>
        <ul class="nav navbar-nav navbar-right">
            <li>
                @Html.ActionLink("Sign out", "SignOut", "Access")
            </li>
        </ul>
    </text>
}
else
{
    <ul class="nav navbar-nav navbar-right">
        <li>@Html.ActionLink("Sign in", "SignIn", "Access", 
           routeValues: null, htmlAttributes: new { id = "loginLink" })</li>
    </ul>
}
